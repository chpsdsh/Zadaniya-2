#include <stdio.h>

int min(int a[4]){
    int min=99999999;
    for(int i=0;i<4;i++){
        if (a[i]<min)
            min=a[i];
    }
    return min;
}
int max(int a[4]) {
    int max = -99999999;
    for (int i = 0; i < 4; i++) {
        if (a[i] > max)
            max = a[i];
    }
    return max;
}
int main() {
    int x1,y1,x2,y2;
    int x3,y3,x4,y4;
    scanf_s("%d %d %d %d",&x1,&y1,&x2,&y2);
    scanf_s("%d %d %d %d",&x3,&y3,&x4,&y4);
    int a[4]={x1,x2,x3,x4};
    int b[4]={y1,y2,y3,y4};
    printf("left bottom %d %d\n",min(a),min(b));
    printf("right top %d %d\n",max(a),max(b));
    return 0;
}
